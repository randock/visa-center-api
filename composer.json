{
  "name": "randock/visa-center-api",
  "description": "Randock Visa Center Api",
  "type": "library",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "Randock\\VisaCenterApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Randock\\VisaCenterApi\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.1"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "randock/utils": "^0.0.2",
    "randock/value-object": "^0.0.9",
    "randock/build": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.4",
    "phpunit/phpunit": "^5.0"
  }
}
